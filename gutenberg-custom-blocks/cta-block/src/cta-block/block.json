{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/cta-block",
	"version": "0.1.0",
	"title": "Call to Action",
	"category": "widgets",
	"icon": "megaphone",
	"description": "Professional call-to-action block with customizable text and button.",
	"attributes": {
		"heading": {
			"type": "string",
			"default": "Ready to Get Started?"
		},
		"description": {
			"type": "string",
			"default": "Join thousands of satisfied customers today."
		},
		"buttonText": {
			"type": "string",
			"default": "Get Started"
		},
		"buttonUrl": {
			"type": "string",
			"default": ""
		}
	},
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		},
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"textdomain": "cta-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}