{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/zacks-react-suite",
	"version": "0.1.0",
	"title": "Marketing CTA Block",
	"category": "widgets",
	"icon": "megaphone",
	"description": "Professional call-to-action block for marketing campaigns",
	"attributes": {
		"heading": {
			"type": "string",
			"default": "Ready to Get Started?"
		},
		"description": {
			"type": "string",
			"default": "Join thousands of satisfied customers today."
		},
		"buttonText": {
			"type": "string",
			"default": "Get Started"
		},
		"buttonUrl": {
			"type": "string",
			"default": ""
		},
		"backgroundColor": {
			"type": "string",
			"default": "#0073aa"
		},
		"textColor": {
			"type": "string",
			"default": "#ffffff"
		}
	},
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		},
		"spacing": {
			"padding": true,
			"margin": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		}
	},
	"textdomain": "zacks-react-suite",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
